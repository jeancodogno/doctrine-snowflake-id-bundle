{
    "name": "jeancodognodev/doctrine-snowflake-id-bundle",
    "description": "Symfony bundle to automatically generate Snowflake IDs",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "symfony/framework-bundle": "^7.2",
        "doctrine/orm": "^3.3",
        "godruoyi/php-snowflake": "^3.1",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/mongodb-odm-bundle": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "JeanCodogno\\DoctrineSnowflakeIdBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jean Carlo Codogno"
        }
    ],
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "nunomaduro/phpinsights": "^2.13",
        "pestphp/pest": "^3.8",
        "mockery/mockery": "^1.6"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1-dev"
        },
        "thanks": {
            "name": "jeancodogno"
        },
        "symfony": {
            "bundle": "JeanCodogno\\DoctrineSnowflakeIdBundle\\DoctrineSnowflakeIdBundle"
        }
    }
}
